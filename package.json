{
  "name": "khalid-portfolio-sveltekit-shadcn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node init-sveltekit.js",
    "dev": "npm run setup && vite dev",
    "dev:alt": "vite --config vite.direct.config.js",
    "build": "npm run setup && vite build",
    "build:alt": "vite build --config vite.direct.config.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-load-config": "^4.0.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "lucide-svelte": "^0.294.0",
    "motion": "^10.16.4"
  }
}